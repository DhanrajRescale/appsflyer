<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/stock_detail_navigation"
    app:startDestination="@+id/stock_detail_fragment">
    <action
        android:id="@+id/sellStock"
        app:destination="@+id/stock_sell_fragment"
        app:enterAnim="@anim/enter_from_right"
        app:exitAnim="@anim/exit_from_left"
        app:popEnterAnim="@anim/enter_from_left"
        app:popExitAnim="@anim/exit_to_right"/>
    <fragment
        android:label="StockDetailFragment"
        android:name="com.assetgro.stockgro.ui.stock.detail.StockDetailFragment"
        android:id="@+id/stock_detail_fragment">
        <argument
            android:name="portfolioId"
            app:argType="string"
            app:nullable="true"/>
        <action
            android:id="@+id/action_stock_detail_fragment_to_stock_order_fragment"
            app:destination="@+id/stock_order_fragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_from_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
        <action
            android:id="@+id/action_stock_detail_fragment_to_stock_short_sell_order_fragment"
            app:destination="@+id/stock_short_sell_order_fragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_from_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
    </fragment>
    <fragment
        android:label="StockDetailFragment"
        android:name="com.assetgro.stockgro.ui.stock.detail.order.StockOrderFragment"
        android:id="@+id/stock_order_fragment">
        <deepLink app:uri="stockgro://stocks/buy"/>
        <action
            android:id="@+id/action_stock_order_fragment_to_fragment_stock_order_confirmation"
            app:destination="@+id/stock_order_confirmation_fragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_from_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
    </fragment>
    <fragment
        android:label="StockShortSellOrderFragment"
        android:name="com.assetgro.stockgro.ui.stock.shortSell.order.StockShortSellOrderFragment"
        android:id="@+id/stock_short_sell_order_fragment">
        <deepLink app:uri="stockgro://stocks/short"/>
        <action
            android:id="@+id/action_stock_short_sell_order_fragment_to_fragment_stock_order_confirmation"
            app:destination="@+id/stock_short_sell_confirmation_fragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_from_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
    </fragment>
    <fragment
        android:label="StockOrderConfirmationFragment"
        android:name="com.assetgro.stockgro.ui.stock.detail.order.StockOrderConfirmationFragment"
        android:id="@+id/stock_order_confirmation_fragment">
        <action
            android:id="@+id/transactionSuccessful"
            app:destination="@+id/stock_payment_success_fragment"
            app:popUpTo="@+id/stock_order_fragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/transactionPending"
            app:destination="@+id/stock_transaction_pending_fragment"
            app:popUpTo="@+id/stock_order_fragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:label="StockOrderConfirmationFragment"
        android:name="com.assetgro.stockgro.ui.stock.shortSell.confirm.StockShortSellConfirmationFragment"
        android:id="@+id/stock_short_sell_confirmation_fragment">
        <action
            android:id="@+id/transactionSuccessful"
            app:destination="@+id/stock_payment_success_fragment"
            app:popUpTo="@+id/stock_order_fragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/transactionPending"
            app:destination="@+id/stock_transaction_pending_fragment"
            app:popUpTo="@+id/stock_order_fragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:label="StockDetailFragment"
        android:name="com.assetgro.stockgro.ui.stock.detail.order.StockOrderPaymentSuccessFragment"
        android:id="@+id/stock_payment_success_fragment">
        <argument
            android:name="stock"
            app:argType="com.assetgro.stockgro.data.model.StockOrderDto"
            app:nullable="false"/>
    </fragment>
    <fragment
        android:label="Sell Stock"
        android:name="com.assetgro.stockgro.ui.stock.sell.StockSellFragment"
        android:id="@+id/stock_sell_fragment">
        <argument
            android:name="portfolioId"
            app:argType="string"
            app:nullable="false"/>
        <argument
            android:name="stockId"
            app:argType="string"
            app:nullable="false"/>
        <argument
            android:name="quantity"
            app:argType="integer"/>
        <deepLink app:uri="stockgro://stocks/sell?portfolioId={portfolioId}&amp;stockId={stockId}&amp;quantity={quantity}"/>
        <action
            android:id="@+id/reviewOrder"
            app:destination="@+id/stock_sell_order_confirmation_fragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_from_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
    </fragment>
    <fragment
        android:label="StockCoverOrderFragment"
        android:name="com.assetgro.stockgro.ui.stock.cover.order.StockCoverOrderFragment"
        android:id="@+id/stock_cover_order_fragment">
        <argument
            android:name="portfolioId"
            app:argType="string"
            app:nullable="false"/>
        <argument
            android:name="stockId"
            app:argType="string"
            app:nullable="false"/>
        <argument
            android:name="quantity"
            app:argType="integer"/>
        <deepLink app:uri="stockgro://stocks/cover?portfolioId={portfolioId}&amp;stockId={stockId}&amp;quantity={quantity}"/>
        <action
            android:id="@+id/reviewCoverOrder"
            app:destination="@+id/stock_cover_order_confirmation_fragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_from_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
    </fragment>
    <fragment
        android:label="Stock Sell Order Confirmation"
        android:name="com.assetgro.stockgro.ui.stock.sell.StockSellOrderConfirmationFragment"
        android:id="@+id/stock_sell_order_confirmation_fragment">
        <argument
            android:name="stockOrder"
            app:argType="com.assetgro.stockgro.data.model.StockOrderDto"
            app:nullable="false"/>
        <action
            android:id="@+id/stockSellTransactionSuccess"
            app:destination="@+id/stock_payment_success_fragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_from_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"
            app:popUpTo="@+id/stock_sell_fragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/stockSellTransactionPending"
            app:destination="@+id/stock_transaction_pending_fragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_from_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"
            app:popUpTo="@+id/stock_sell_fragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:label="StockCoverOrderConfirmationFragment"
        android:name="com.assetgro.stockgro.ui.stock.cover.review.StockCoverOrderConfirmationFragment"
        android:id="@+id/stock_cover_order_confirmation_fragment">
        <argument
            android:name="stockOrder"
            app:argType="com.assetgro.stockgro.data.model.StockOrderDto"
            app:nullable="false"/>
        <action
            android:id="@+id/stockSellTransactionSuccess"
            app:destination="@+id/stock_payment_success_fragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_from_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"
            app:popUpTo="@+id/stock_sell_fragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/stockSellTransactionPending"
            app:destination="@+id/stock_transaction_pending_fragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_from_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"
            app:popUpTo="@+id/stock_sell_fragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:label="Transaction Pending"
        android:name="com.assetgro.stockgro.ui.stock.common.StockTransactionPendingFragment"
        android:id="@+id/stock_transaction_pending_fragment">
        <argument
            android:name="stockOrder"
            app:argType="com.assetgro.stockgro.data.model.StockOrderDto"
            app:nullable="false"/>
    </fragment>
</navigation>
