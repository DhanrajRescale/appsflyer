<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/fragment_fno_option_detail_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.google.android.material.appbar.AppBarLayout
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
        android:id="@+id/app_bar_layout"
        android:background="@drawable/collapsing_app_bar_background"
        android:fitsSystemWindows="true"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:elevation="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="@dimen/collapsed_toolbar_height"
                android:elevation="0dp"
                app:contentInsetStartWithNavigation="0dp"
                app:layout_collapseMode="pin"
                app:navigationIcon="@drawable/ic_back"
                app:subtitleTextAppearance="@style/CollapsedAppBar"
                app:titleTextAppearance="@style/CollapsedAppBar"
                app:titleTextColor="@color/toolbarTitleColor">
                <com.assetgro.stockgro.widget.MarketAssetToolbarView
                    android:id="@+id/header"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
    </com.google.android.material.appbar.AppBarLayout>
    <ProgressBar
        android:id="@+id/loading_progress_bar"
        android:tag="binding_1"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/app_bar_layout"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/option_overview_container"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/app_bar_layout">
        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/secondary_header"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp">
                <TextView
                    android:textSize="14dp"
                    android:textColor="@color/black_1A1A1A"
                    android:ellipsize="end"
                    android:id="@+id/option_name"
                    android:tag="binding_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:lines="1"
                    android:fontFamily="@font/inter_500"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/show_ohlc"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="14dp"
                    android:textColor="@color/black_1A1A1A"
                    android:id="@+id/ltp"
                    android:tag="binding_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_600"
                    android:textAlignment="textEnd"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/option_name"/>
                <ImageView
                    android:id="@+id/show_ohlc"
                    android:padding="8dp"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:src="@drawable/ic_down_arrow"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintBottom_toBottomOf="@+id/ltp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/option_name"/>
                <com.assetgro.stockgro.widget.StockChangeInfoView
                    android:id="@+id/contract_change_view"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    app:layout_constraintBottom_toBottomOf="@+id/ltp"
                    app:layout_constraintStart_toEndOf="@+id/ltp"
                    app:layout_constraintTop_toTopOf="@+id/ltp"/>
                <androidx.legacy.widget.Space
                    android:id="@+id/ohlc_top_space"
                    android:layout_width="0dp"
                    android:layout_height="10dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/contract_change_view"/>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/ohlc_container"
                    android:tag="binding_4"
                    android:background="@color/white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ltp"
                    app:layout_goneMarginTop="16dp">
                    <ProgressBar
                        android:id="@+id/olhc_loader"
                        android:visibility="gone"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        android:layout_marginTop="16dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <View
                        android:id="@+id/ohlc_divider_one"
                        android:background="@color/black_1A1A1A_10"
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_marginTop="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/black_1A1A1A_40"
                        android:id="@+id/open_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:text="@string/open_text"
                        android:fontFamily="@font/inter_500"
                        app:layout_constraintEnd_toStartOf="@+id/high_title"
                        app:layout_constraintHorizontal_chainStyle="spread"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/ohlc_divider_one"/>
                    <TextView
                        android:textSize="14dp"
                        android:textColor="@color/black_1A1A1A"
                        android:id="@+id/open"
                        android:tag="binding_5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:fontFamily="@font/inter_500"
                        app:layout_constraintEnd_toEndOf="@+id/open_title"
                        app:layout_constraintStart_toStartOf="@+id/open_title"
                        app:layout_constraintTop_toBottomOf="@+id/open_title"/>
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/black_1A1A1A_40"
                        android:id="@+id/high_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/high_text"
                        android:fontFamily="@font/inter_500"
                        app:layout_constraintEnd_toStartOf="@+id/low_title"
                        app:layout_constraintHorizontal_chainStyle="spread"
                        app:layout_constraintStart_toEndOf="@+id/open_title"
                        app:layout_constraintTop_toTopOf="@+id/open_title"/>
                    <TextView
                        android:textSize="14dp"
                        android:textColor="@color/black_1A1A1A"
                        android:id="@+id/high"
                        android:tag="binding_6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:fontFamily="@font/inter_500"
                        app:layout_constraintEnd_toEndOf="@+id/high_title"
                        app:layout_constraintStart_toStartOf="@+id/high_title"
                        app:layout_constraintTop_toBottomOf="@+id/high_title"/>
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/black_1A1A1A_40"
                        android:id="@+id/low_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/low_text"
                        android:fontFamily="@font/inter_500"
                        app:layout_constraintEnd_toStartOf="@+id/prev_close_title"
                        app:layout_constraintHorizontal_chainStyle="spread"
                        app:layout_constraintStart_toEndOf="@+id/high_title"
                        app:layout_constraintTop_toTopOf="@+id/open_title"/>
                    <TextView
                        android:textSize="14dp"
                        android:textColor="@color/black_1A1A1A"
                        android:id="@+id/low"
                        android:tag="binding_7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:fontFamily="@font/inter_500"
                        app:layout_constraintEnd_toEndOf="@+id/low_title"
                        app:layout_constraintStart_toStartOf="@+id/low_title"
                        app:layout_constraintTop_toBottomOf="@+id/low_title"/>
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/black_1A1A1A_40"
                        android:id="@+id/prev_close_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/prev_close"
                        android:fontFamily="@font/inter_500"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_chainStyle="spread"
                        app:layout_constraintStart_toEndOf="@+id/low_title"
                        app:layout_constraintTop_toTopOf="@+id/open_title"/>
                    <TextView
                        android:textSize="14dp"
                        android:textColor="@color/black_1A1A1A"
                        android:id="@+id/prev_close"
                        android:tag="binding_8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:fontFamily="@font/inter_500"
                        app:layout_constraintEnd_toEndOf="@+id/prev_close_title"
                        app:layout_constraintStart_toStartOf="@+id/prev_close_title"
                        app:layout_constraintTop_toBottomOf="@+id/prev_close_title"/>
                    <View
                        android:id="@+id/ohlc_divider_two"
                        android:background="@color/black_1A1A1A_10"
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_marginTop="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/open"/>
                    <com.github.mikephil.charting.charts.LineChart
                        android:id="@+id/history_chart"
                        android:layout_width="0dp"
                        android:layout_height="164dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginHorizontal="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/ohlc_divider_two"/>
                    <TextView
                        android:textSize="10dp"
                        android:textColor="@color/black_1A1A1A"
                        android:id="@+id/no_history_data_message"
                        android:visibility="gone"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="32dp"
                        android:layout_marginRight="32dp"
                        android:text="@string/no_chart_data_available"
                        android:fontFamily="@font/inter_400"
                        android:textAlignment="center"
                        android:layout_marginHorizontal="32dp"
                        app:layout_constraintBottom_toBottomOf="@+id/history_chart"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/history_chart"/>
                    <com.google.android.material.chip.ChipGroup
                        android:id="@+id/history_data_selector"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginStart="16dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/history_chart"
                        app:singleLine="true"
                        app:singleSelection="true">
                        <com.google.android.material.chip.Chip
                            android:id="@+id/one_day"
                            android:tag="one_day"
                            android:layout_width="wrap_content"
                            android:layout_height="40dp"
                            android:checked="true"
                            android:text="@string/one_day"
                            android:checkable="true"
                            style="@style/ChipStyleNew"/>
                        <com.google.android.material.chip.Chip
                            android:id="@+id/one_week"
                            android:tag="one_week"
                            android:layout_width="wrap_content"
                            android:layout_height="40dp"
                            android:checked="false"
                            android:text="@string/one_week"
                            android:checkable="true"
                            style="@style/ChipStyleNew"/>
                    </com.google.android.material.chip.ChipGroup>
                    <ProgressBar
                        android:tag="binding_9"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        android:layout_marginTop="32dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/ohlc_divider_two"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.appbar.AppBarLayout>
        <androidx.core.widget.NestedScrollView
            android:id="@+id/nestedScrollView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:fillViewport="true"
            app:layout_behavior="@string/appbar_scrolling_view_behavior"
            app:layout_constraintBottom_toTopOf="@+id/action_button_container"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/secondary_header"
            app:layout_constraintVertical_bias="0">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/blue_3417A4"
                    android:id="@+id/break_even_chart_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="@string/breakeven_chart"
                    android:fontFamily="@font/inter_500"
                    android:layout_marginStart="16dp"
                    app:layout_constraintBottom_toTopOf="@+id/break_even_chart"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <ImageView
                    android:id="@+id/narration_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_info_field_large"
                    android:layout_marginStart="8dp"
                    app:layout_constraintBottom_toBottomOf="@+id/break_even_chart_title"
                    app:layout_constraintStart_toEndOf="@+id/break_even_chart_title"
                    app:layout_constraintTop_toTopOf="@+id/break_even_chart_title"/>
                <TextView
                    android:textSize="12dp"
                    android:id="@+id/switch_buy_text"
                    android:tag="binding_10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/buy"
                    android:fontFamily="@font/inter_500"
                    app:layout_constraintBottom_toBottomOf="@+id/call_put_switch"
                    app:layout_constraintEnd_toStartOf="@+id/call_put_switch"
                    app:layout_constraintTop_toTopOf="@+id/call_put_switch"/>
                <androidx.appcompat.widget.SwitchCompat
                    android:id="@+id/call_put_switch"
                    android:tag="binding_11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="@+id/break_even_chart_title"
                    app:layout_constraintEnd_toStartOf="@+id/switch_sell_text"
                    app:layout_constraintTop_toTopOf="@+id/break_even_chart_title"
                    app:thumbTint="@color/white"
                    app:track="@drawable/option_call_put_track"/>
                <TextView
                    android:textSize="12dp"
                    android:id="@+id/switch_sell_text"
                    android:tag="binding_12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/sell"
                    android:fontFamily="@font/inter_500"
                    android:layout_marginEnd="16dp"
                    app:layout_constraintBottom_toBottomOf="@+id/call_put_switch"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/call_put_switch"/>
                <com.github.mikephil.charting.charts.LineChart
                    android:id="@+id/break_even_chart"
                    android:background="@color/white"
                    android:layout_width="0dp"
                    android:layout_height="170dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginStart="16dp"
                    android:nestedScrollingEnabled="false"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/break_even_chart_title"/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/black_1A1A1A_40"
                    android:id="@+id/break_even_point_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/breakeven_point"
                    android:fontFamily="@font/inter_400"
                    android:textAlignment="textStart"
                    android:layout_marginStart="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/break_even_chart"/>
                <TextView
                    android:textSize="14dp"
                    android:textColor="@color/deep_purple"
                    android:id="@+id/break_even_point"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:fontFamily="@font/inter_600"
                    app:layout_constraintStart_toStartOf="@+id/break_even_point_title"
                    app:layout_constraintTop_toBottomOf="@+id/break_even_point_title"/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/black_1A1A1A_40"
                    android:id="@+id/expected_pl_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/expected_pl"
                    android:fontFamily="@font/inter_400"
                    android:textAlignment="textEnd"
                    android:layout_marginEnd="16dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/break_even_chart"/>
                <TextView
                    android:textSize="14dp"
                    android:textColor="@color/black_1A1A1A"
                    android:id="@+id/expected_pl"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:text="--"
                    android:fontFamily="@font/inter_600"
                    android:textAlignment="textEnd"
                    app:layout_constraintEnd_toEndOf="@+id/expected_pl_title"
                    app:layout_constraintTop_toBottomOf="@+id/break_even_point_title"/>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/break_even_chart_not_plottable_container"
                    android:visibility="gone"
                    android:layout_width="0dp"
                    android:layout_height="170dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginRight="16dp"
                    android:layout_marginHorizontal="16dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/break_even_chart_title">
                    <ImageView
                        android:id="@+id/break_even_chart_not_plottable_illustration"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/break_even_chart_not_plottable_illustration"
                        app:layout_constraintBottom_toTopOf="@+id/break_even_chart_not_plottable_message"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_chainStyle="packed"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                        android:textSize="10dp"
                        android:textColor="@color/black_1A1A1A"
                        android:id="@+id/break_even_chart_not_plottable_message"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="32dp"
                        android:layout_marginRight="32dp"
                        android:text="@string/break_even_chart_not_plottable_message"
                        android:fontFamily="@font/inter_400"
                        android:textAlignment="center"
                        android:layout_marginHorizontal="32dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_chainStyle="packed"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/break_even_chart_not_plottable_illustration"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/expiry_date_container"
                    android:background="@color/white"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/expected_pl">
                    <View
                        android:id="@+id/expiry_date_separator"
                        android:background="@color/screenBackground"
                        android:layout_width="0dp"
                        android:layout_height="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                        android:textSize="14dp"
                        android:textColor="@color/black_1A1A1A"
                        android:id="@+id/expiry_date_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:text="@string/expiry_date"
                        android:fontFamily="@font/inter_400"
                        android:layout_marginStart="16dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/expiry_date_separator"/>
                    <HorizontalScrollView
                        android:scrollbars="none"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/expiry_date_title">
                        <com.google.android.material.chip.ChipGroup
                            android:id="@+id/expiry_date_chip_group"
                            android:layout_width="wrap_content"
                            android:layout_height="40dp"
                            android:paddingEnd="32dp"
                            android:layout_marginStart="16dp"
                            android:layout_marginEnd="16dp"
                            app:singleLine="true"
                            app:singleSelection="true"/>
                    </HorizontalScrollView>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/strike_price_container"
                    android:background="@color/white"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/expiry_date_container">
                    <View
                        android:id="@+id/strike_price_separator"
                        android:background="@color/screenBackground"
                        android:layout_width="0dp"
                        android:layout_height="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                        android:textSize="14dp"
                        android:textColor="@color/black_1A1A1A"
                        android:id="@+id/strike_price_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="16dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginRight="16dp"
                        android:text="@string/strike_price"
                        android:fontFamily="@font/inter_400"
