<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/stock_nested_scroll_view"
    android:tag="layout/cell_stock_analysis_0"
    android:background="#f1eeee"
    android:visibility="visible"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:textSize="16dp"
            android:textColor="@color/black"
            android:id="@+id/approach_title"
            android:tag="binding_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="16dp"
            android:fontFamily="@font/inter_600"
            android:layout_marginHorizontal="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <ImageView
            android:id="@+id/approach_narration_icon"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_info_field_large"
            android:layout_marginStart="6dp"
            app:layout_constraintBottom_toBottomOf="@+id/approach_title"
            app:layout_constraintStart_toEndOf="@+id/approach_title"
            app:layout_constraintTop_toTopOf="@+id/approach_title"/>
        <TextView
            android:textSize="12dp"
            android:id="@+id/calculated_value"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginHorizontal="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/approach_title"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:tag="binding_2"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <ImageView
                android:id="@+id/no_data_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:src="@drawable/ic_market_section_no_data"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="14dp"
                android:textColor="@color/black"
                android:id="@+id/no_data_description"
                android:tag="binding_3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="16dp"
                android:layout_marginRight="24dp"
                android:layout_marginBottom="24dp"
                android:alpha="0.6"
                android:fontFamily="@font/inter_400"
                android:textAlignment="center"
                android:layout_marginHorizontal="24dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/no_data_icon"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.Group
            android:tag="binding_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:constraint_referenced_ids="calculated_value_container,market_slider_1,market_slider_2,market_slider_3"
            app:layout_goneMarginTop="16dp"/>
        <androidx.cardview.widget.CardView
            android:id="@+id/calculated_value_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginHorizontal="16dp"
            app:cardCornerRadius="10dp"
            app:layout_constraintTop_toBottomOf="@+id/calculated_value"
            app:layout_goneMarginTop="16dp">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:padding="16dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:layout_gravity="center_horizontal"
                    android:id="@+id/gauge_container"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">
                    <ImageView
                        android:id="@+id/ic_stock_analysis_v2"
                        android:layout_width="wrap_content"
                        android:layout_height="96dp"
                        android:src="@drawable/ic_stock_analysis_v2"
                        app:elevation="2dp"/>
                    <ImageView
                        android:id="@+id/needle"
                        android:layout_width="wrap_content"
                        android:layout_height="76dp"
                        android:src="@drawable/ic_market_analytics_webp_4x"
                        android:layout_centerHorizontal="true"
                        app:elevation="8dp"/>
                    <ImageView
                        android:id="@+id/needle_pivot"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="4dp"
                        android:src="@drawable/ic_needle_stub"
                        android:layout_alignBottom="@+id/needle"
                        android:layout_centerHorizontal="true"
                        app:elevation="8dp"/>
                </RelativeLayout>
                <TextView
                    android:textSize="8dp"
                    android:textColor="@color/black"
                    android:id="@+id/fair_value_title"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:text="@string/fair_value"
                    android:textAllCaps="true"
                    app:layout_constraintBottom_toTopOf="@+id/fair_value"
                    app:layout_constraintEnd_toEndOf="@+id/gauge_container"
                    app:layout_constraintStart_toStartOf="@+id/gauge_container"/>
                <TextView
                    android:textSize="12dp"
                    android:id="@+id/fair_value"
                    android:tag="binding_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:fontFamily="@font/inter_500"
                    app:layout_constraintEnd_toEndOf="@+id/gauge_container"
                    app:layout_constraintStart_toStartOf="@+id/gauge_container"
                    app:layout_constraintTop_toBottomOf="@+id/gauge_container"/>
                <TextView
                    android:textSize="8dp"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:text="@string/undervalued"
                    android:layout_marginStart="-8dp"
                    app:layout_constraintStart_toStartOf="@+id/gauge_container"
                    app:layout_constraintTop_toBottomOf="@+id/gauge_container"/>
                <TextView
                    android:textSize="8dp"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:text="@string/overvalued"
                    android:layout_marginEnd="-8dp"
                    app:layout_constraintEnd_toEndOf="@+id/gauge_container"
                    app:layout_constraintTop_toBottomOf="@+id/gauge_container"/>
                <TextView
                    android:textSize="10dp"
                    android:textColor="@color/black"
                    android:id="@+id/market_price_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:text="@string/market_price"
                    android:alpha="0.8"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/fair_value"/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/black"
                    android:id="@+id/market_price"
                    android:tag="binding_6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:textAllCaps="true"
                    android:fontFamily="@font/inter_600"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/market_price_title"/>
                <TextView
                    android:textSize="10dp"
                    android:textColor="@color/black"
                    android:id="@+id/discount_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:text="@string/fair_value"
                    android:alpha="0.8"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/fair_value"/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/black"
                    android:id="@+id/discount"
                    android:tag="binding_7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:textAllCaps="true"
                    android:fontFamily="@font/inter_600"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/discount_title"/>
                <TextView
                    android:textSize="12dp"
                    android:textColor="@color/black"
                    android:id="@+id/valuation_verdict"
                    android:tag="binding_8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_600"
                    app:layout_constraintEnd_toStartOf="@+id/valuation_icon"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/fair_value"/>
                <ImageView
                    android:id="@+id/valuation_icon"
                    android:tag="binding_9"
                    android:layout_width="14dp"
                    android:layout_height="27dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="8dp"
                    app:layout_constraintBottom_toBottomOf="@+id/valuation_verdict"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toEndOf="@+id/valuation_verdict"
                    app:layout_constraintTop_toTopOf="@+id/valuation_verdict"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>
        <com.assetgro.stockgro.widget.MarketSlider
            android:id="@+id/market_slider_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="12dp"
            android:layout_marginRight="16dp"
            android:layout_marginHorizontal="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/calculated_value_container"/>
        <com.assetgro.stockgro.widget.MarketSlider
            android:id="@+id/market_slider_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="8dp"
            android:layout_marginRight="16dp"
            android:layout_marginHorizontal="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/market_slider_1"/>
        <com.assetgro.stockgro.widget.MarketSlider
            android:id="@+id/market_slider_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="8dp"
            android:layout_marginRight="16dp"
            android:layout_marginHorizontal="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/market_slider_2"/>
        <androidx.legacy.widget.Space
            android:layout_width="match_parent"
            android:layout_height="98dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/market_slider_3"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>
