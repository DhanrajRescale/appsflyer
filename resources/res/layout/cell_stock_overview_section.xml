<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/cell_stock_overview_section_0"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:textSize="18dp"
        android:textColor="@color/black_1A1A1A"
        android:id="@+id/section_title"
        android:tag="binding_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/inter_600"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="96dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
        android:id="@+id/section_sentiment"
        android:tag="binding_2"
        android:layout_width="18dp"
        android:layout_height="24dp"
        android:layout_marginTop="4dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/section_title"
        app:layout_constraintStart_toEndOf="@+id/section_title"
        app:layout_constraintTop_toTopOf="@+id/section_title"/>
    <androidx.appcompat.widget.SwitchCompat
        android:layout_gravity="center_horizontal"
        android:id="@+id/representation_switch"
        android:tag="binding_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:thumb="@drawable/data_chart_thumb"
        android:scaleX="1.3"
        android:scaleY="1.3"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/section_title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/section_title"
        app:showText="false"
        app:switchMinWidth="64dp"
        app:track="@drawable/data_chart_track"/>
    <TextView
        android:textSize="10dp"
        android:textColor="@color/screenBackground"
        android:id="@+id/chart_switch_title"
        android:tag="binding_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/chart"
        android:textAllCaps="true"
        android:fontFamily="@font/inter_500"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/representation_switch"
        app:layout_constraintEnd_toEndOf="@+id/representation_switch"
        app:layout_constraintTop_toTopOf="@+id/representation_switch"/>
    <TextView
        android:textSize="10dp"
        android:textColor="@color/screenBackground"
        android:id="@+id/data_switch_title"
        android:tag="binding_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/data"
        android:textAllCaps="true"
        android:fontFamily="@font/inter_500"
        android:textAlignment="center"
        android:layout_marginStart="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/representation_switch"
        app:layout_constraintStart_toStartOf="@+id/representation_switch"
        app:layout_constraintTop_toTopOf="@+id/representation_switch"/>
    <LinearLayout
        android:orientation="vertical"
        android:tag="binding_6"
        android:background="@color/white"
        android:paddingBottom="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/section_title">
        <com.github.mikephil.charting.charts.BarChart
            android:tag="binding_7"
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginHorizontal="16dp"/>
        <com.github.mikephil.charting.charts.PieChart
            android:tag="binding_8"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="24dp"
            android:layout_marginHorizontal="24dp"/>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/tabular_data"
            android:tag="binding_9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/headers"
                android:tag="binding_10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <LinearLayout
                android:orientation="vertical"
                android:tag="binding_11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:paddingHorizontal="16dp">
            <ImageView
                android:id="@+id/narration_icon"
                android:tag="binding_12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_info_field_large"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="12dp"
                android:textColor="@color/black"
                android:id="@+id/narration_title"
                android:tag="binding_13"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/inter_400"
                android:layout_marginStart="4dp"
                app:layout_constraintBottom_toBottomOf="@+id/narration_icon"
                app:layout_constraintEnd_toStartOf="@+id/description"
                app:layout_constraintStart_toEndOf="@+id/narration_icon"
                app:layout_constraintTop_toTopOf="@+id/narration_icon"/>
            <TextView
                android:textSize="12dp"
                android:textColor="@color/black"
                android:id="@+id/description"
                android:tag="binding_14"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:alpha="0.6"
                android:fontFamily="@font/inter_400"
                android:textAlignment="textEnd"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/narration_title"
                app:layout_constraintTop_toTopOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:tag="binding_15"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/section_title">
        <ImageView
            android:id="@+id/no_data_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:src="@drawable/ic_market_section_no_data"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:textSize="14dp"
            android:textColor="@color/black"
            android:id="@+id/no_data_description"
            android:tag="binding_16"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="24dp"
            android:layout_marginBottom="24dp"
            android:alpha="0.6"
            android:fontFamily="@font/inter_400"
            android:textAlignment="center"
            android:layout_marginHorizontal="24dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/no_data_icon"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
