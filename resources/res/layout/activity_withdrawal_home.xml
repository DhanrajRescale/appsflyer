<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/activity_withdrawal_home_0"
    android:background="@color/screenBackground"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.google.android.material.appbar.AppBarLayout
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
        android:background="@android:color/transparent"
        android:fitsSystemWindows="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:elevation="0dp">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:background="@drawable/collapsing_app_bar_background"
            android:layout_width="match_parent"
            android:layout_height="@dimen/collapsed_toolbar_height"
            android:minHeight="@dimen/collapsed_toolbar_height"
            android:elevation="0dp"
            app:layout_collapseMode="pin"
            app:navigationIcon="@drawable/ic_back_button"
            app:titleTextColor="@color/toolbarTitleColor"/>
    </com.google.android.material.appbar.AppBarLayout>
    <androidx.core.widget.NestedScrollView
        android:tag="binding_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.cardview.widget.CardView
                android:id="@+id/payment_methods"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="24dp"
                android:layout_marginRight="16dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="@dimen/default_card_radius"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:background="@drawable/rounded_sky_blue_rectangle_corner_radius_16dp"
                    android:padding="24dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textSize="18dp"
                        android:textStyle="bold"
                        android:textColor="@color/black_1A1A1A"
                        android:id="@+id/transfer_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="16dp"
                        android:text="@string/text_transfer_to"
                        android:layout_centerVertical="true"
                        android:fontFamily="@font/inter_600"
                        app:layout_constraintBottom_toTopOf="@+id/option_withdrawal_paytm_wallet"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:orientation="horizontal"
                        android:id="@+id/option_withdrawal_paytm_wallet"
                        android:tag="binding_2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginBottom="16dp"
                        android:layout_marginVertical="16dp"
                        app:layout_constraintBottom_toTopOf="@+id/separator_one"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/transfer_label"
                        app:layout_goneMarginBottom="12dp">
                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/wallet_icon"
                            android:tag="binding_3"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:fontFamily="@font/inter_500"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <TextView
                            android:textSize="15dp"
                            android:textColor="@color/black_1A1A1A"
                            android:tag="binding_4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/text_paytm"
                            android:layout_centerVertical="true"
                            android:fontFamily="@font/inter_500"
                            android:layout_marginStart="16dp"
                            app:layout_constraintBottom_toBottomOf="@+id/wallet_icon"
                            app:layout_constraintStart_toEndOf="@+id/wallet_icon"
                            app:layout_constraintTop_toTopOf="@+id/wallet_icon"/>
                        <ImageView
                            android:tag="binding_5"
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/ic_arrow_right"
                            android:layout_centerVertical="true"
                            android:fontFamily="@font/inter_500"
                            android:layout_alignParentEnd="true"
                            app:layout_constraintBottom_toBottomOf="@+id/wallet_icon"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/wallet_icon"/>
                        <include
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp"
                            android:layout_marginBottom="16dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/wallet_icon"
                            layout="@layout/layout_payment_error_child_view"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <View
                        android:id="@+id/separator_one"
                        android:tag="binding_6"
                        android:background="@color/skyBlueCardSeparator"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        app:layout_constraintBottom_toTopOf="@+id/option_withdrawal_bank"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/option_withdrawal_paytm_wallet"/>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:orientation="horizontal"
                        android:id="@+id/option_withdrawal_bank"
                        android:tag="binding_7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginBottom="16dp"
                        android:layout_marginVertical="16dp"
                        app:layout_constraintBottom_toTopOf="@+id/separator_2"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/separator_one"
                        app:layout_goneMarginTop="16dp">
                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/bank_icon"
                            android:tag="binding_8"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:fontFamily="@font/inter_500"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <TextView
                            android:textSize="15dp"
                            android:textColor="@color/black_1A1A1A"
                            android:tag="binding_9"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/text_bank"
                            android:layout_centerVertical="true"
                            android:fontFamily="@font/inter_500"
                            android:layout_marginStart="16dp"
                            app:layout_constraintBottom_toBottomOf="@+id/bank_icon"
                            app:layout_constraintStart_toEndOf="@+id/bank_icon"
                            app:layout_constraintTop_toTopOf="@+id/bank_icon"/>
                        <ImageView
                            android:tag="binding_10"
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/ic_arrow_right"
                            android:layout_centerVertical="true"
                            android:fontFamily="@font/inter_500"
                            android:layout_alignParentEnd="true"
                            app:layout_constraintBottom_toBottomOf="@+id/bank_icon"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/bank_icon"/>
                        <include
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp"
                            android:layout_marginBottom="16dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/bank_icon"
                            layout="@layout/layout_payment_error_child_view"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <View
                        android:id="@+id/separator_2"
                        android:tag="binding_11"
                        android:background="@color/skyBlueCardSeparator"
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        app:layout_constraintBottom_toTopOf="@+id/option_withdrawal_upi"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/option_withdrawal_bank"/>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:orientation="horizontal"
                        android:id="@+id/option_withdrawal_upi"
                        android:tag="binding_12"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginBottom="16dp"
                        android:layout_marginVertical="16dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/separator_2"
                        app:layout_goneMarginTop="16dp">
                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/upi_icon"
                            android:tag="binding_13"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_marginBottom="24dp"
                            android:fontFamily="@font/inter_500"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <TextView
                            android:textSize="15dp"
                            android:textColor="@color/black_1A1A1A"
                            android:tag="binding_14"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/text_upi"
                            android:layout_centerVertical="true"
                            android:fontFamily="@font/inter_500"
                            android:layout_marginStart="16dp"
                            app:layout_constraintBottom_toBottomOf="@+id/upi_icon"
                            app:layout_constraintStart_toEndOf="@+id/upi_icon"
                            app:layout_constraintTop_toTopOf="@+id/upi_icon"/>
                        <ImageView
                            android:tag="binding_15"
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/ic_arrow_right"
                            android:layout_centerVertical="true"
                            android:fontFamily="@font/inter_500"
                            android:layout_alignParentEnd="true"
                            app:layout_constraintBottom_toBottomOf="@+id/upi_icon"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/upi_icon"/>
                        <include
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/upi_icon"
                            layout="@layout/layout_payment_error_child_view"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
            <ImageView
                android:id="@+id/ic_image"
                android:layout_width="112dp"
                android:layout_height="112dp"
                android:layout_marginTop="32dp"
                android:src="@drawable/ic_bull_arrow"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/payment_methods"/>
            <TextView
                android:textSize="14dp"
                android:textColor="@color/black_1A1A1A_60"
                android:gravity="center"
                android:id="@+id/message"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="14dp"
                android:layout_marginRight="16dp"
                android:text="@string/text_guideline_narration_wallet"
                android:fontFamily="@font/inter_500"
                android:layout_marginHorizontal="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/ic_image"/>
            <androidx.legacy.widget.Space
                android:layout_width="0dp"
                android:layout_height="64dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/message"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
