<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/cell_league_upcoming_title_0"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="2dp"
    android:layout_marginTop="2dp"
    android:layout_marginRight="2dp"
    android:layout_marginBottom="16dp"
    android:layout_marginHorizontal="2dp"
    app:cardCornerRadius="12dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/background_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:textSize="14dp"
            android:textColor="@color/leagueNameTitleColor"
            android:id="@+id/league_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:lines="1"
            android:fontFamily="@font/inter_600"
            android:textAlignment="center"
            android:layout_marginStart="4dp"
            app:layout_constraintStart_toEndOf="@+id/league_icon"
            app:layout_constraintTop_toTopOf="@+id/league_icon"/>
        <ImageView
            android:layout_gravity="center_horizontal"
            android:id="@+id/league_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:layout_marginStart="14dp"
            app:crownLocation="none"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_league_badge_v2"/>
        <TextView
            android:textSize="10dp"
            android:textColor="@color/leagueNameSubTitleColor"
            android:id="@+id/league_start_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_below="@+id/league_name"
            android:fontFamily="@font/inter_500"
            app:layout_constraintBottom_toBottomOf="@+id/league_icon"
            app:layout_constraintStart_toStartOf="@+id/league_name"
            app:layout_constraintTop_toBottomOf="@+id/league_name"/>
        <com.assetgro.stockgro.widget.ChipView
            android:textSize="10dp"
            android:textColor="@color/white"
            android:id="@+id/duration"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:textAllCaps="true"
            android:fontFamily="@font/inter_600"
            android:paddingStart="8dp"
            android:paddingEnd="8dp"
            app:layout_constraintStart_toStartOf="@+id/league_start_time"
            app:layout_constraintTop_toBottomOf="@+id/league_start_time"/>
        <com.assetgro.stockgro.widget.ChipView
            android:textSize="10dp"
            android:textColor="@color/white"
            android:id="@+id/league_type"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            android:fontFamily="@font/inter_600"
            android:paddingStart="8dp"
            android:paddingEnd="8dp"
            android:layout_marginStart="2dp"
            app:layout_constraintBottom_toBottomOf="@+id/duration"
            app:layout_constraintStart_toEndOf="@+id/duration"
            app:layout_constraintTop_toTopOf="@+id/duration"/>
        <TextView
            android:textSize="11dp"
            android:textColor="#b3603a29"
            android:layout_gravity="center_horizontal"
            android:id="@+id/prize_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/prize"
            android:fontFamily="@font/inter_500"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/league_type"/>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:gravity="center_vertical"
            android:layout_gravity="center_horizontal"
            android:orientation="horizontal"
            android:id="@+id/linearLayoutCompat2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            app:layout_constraintEnd_toEndOf="@+id/prize_text"
            app:layout_constraintStart_toStartOf="@+id/prize_text"
            app:layout_constraintTop_toBottomOf="@+id/prize_text">
            <ImageView
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginEnd="2dp"
                app:srcCompat="@drawable/ic_stockgro_currency_violet"/>
            <TextView
                android:textSize="12dp"
                android:textStyle="bold"
                android:textColor="@color/leagueNameTitleColor"
                android:id="@+id/total_price_money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/inter_600"/>
        </androidx.appcompat.widget.LinearLayoutCompat>
        <TextView
            android:textSize="18dp"
            android:textStyle="bold"
            android:textColor="#2656ff"
            android:gravity="center"
            android:id="@+id/special_gift_chance"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:fontFamily="@font/inter_600"
            android:paddingStart="60dp"
            android:paddingEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayoutCompat2"/>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/player_min_max"
            android:paddingLeft="16dp"
            android:paddingTop="8dp"
            android:paddingRight="16dp"
            android:paddingBottom="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:paddingHorizontal="16dp"
            android:paddingVertical="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/special_gift_chance"
            app:layout_goneMarginTop="16dp">
            <ProgressBar
                android:id="@+id/seat_left_progress"
                android:layout_width="match_parent"
                android:layout_height="8dp"
                android:progressDrawable="@drawable/progress_drawable_v2"
                style="?android:attr/progressBarStyleHorizontal"/>
            <RelativeLayout
                android:id="@+id/seat_status_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp">
                <LinearLayout
                    android:orientation="horizontal"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        app:srcCompat="@drawable/ic_player"/>
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/black_1A1A1A"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/min"
                        android:fontFamily="@font/inter_500"
                        android:layout_marginStart="4dp"/>
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/black_1A1A1A"
                        android:id="@+id/min_player"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/inter_500"
                        android:layout_marginStart="4dp"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:visibility="visible"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true">
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/leagueNameSubTitleColor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/max"
                        android:fontFamily="@font/inter_500"
                        android:layout_marginStart="4dp"/>
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/black_1A1A1A"
                        android:id="@+id/max_player"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/inter_500"
                        android:layout_marginStart="4dp"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:visibility="visible"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/leagueNameSubTitleColor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Entries:"
                        android:fontFamily="@font/inter_500"
                        android:layout_marginStart="4dp"/>
                    <TextView
                        android:textSize="12dp"
                        android:textColor="@color/black_1A1A1A"
                        android:id="@+id/seat_left"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/inter_500"
                        android:layout_marginStart="4dp"/>
                </LinearLayout>
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/footer"
            android:background="@drawable/rounded_color_light_white_rectangle_corner_radius_16dp"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/player_min_max"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="14dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/player_min_max">
            <TextView
                android:textSize="11dp"
                android:textColor="@color/orange"
                android:id="@+id/days_left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/inter_600"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:gravity="center_vertical"
                android:layout_gravity="center_horizontal"
                android:orientation="horizontal"
                android:id="@+id/cash_left_container"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <TextView
                    android:textSize="10dp"
                    android:textColor="@color/black_1A1A1A"
                    android:layout_gravity="center_horizontal"
                    android:id="@+id/entry_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/entry_fee"
                    android:fontFamily="@font/inter_400"
                    app:layout_constraintEnd_toEndOf="@+id/rank_text"
                    app:layout_constraintTop_toBottomOf="@+id/rank_text"/>
                <ImageView
                    android:layout_width="14dp"
                    android:layout_height="14dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginEnd="2dp"
                    app:srcCompat="@drawable/ic_stockgro_currency_violet"/>
                <TextView
                    android:textSize="10dp"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:id="@+id/entry_amount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_600"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>
