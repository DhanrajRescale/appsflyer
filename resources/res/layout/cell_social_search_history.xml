<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/cell_social_search_history_0"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/ic_search_history"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/ic_search_history"
        android:layout_marginStart="20dp"
        android:layout_marginVertical="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:tint="@color/black_1A1A1A_40"/>
    <TextView
        android:textSize="12dp"
        android:textColor="@color/black_1A1A1A_40"
        android:id="@+id/search_query_text"
        android:tag="binding_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_500"
        android:layout_marginStart="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/ic_search_history"
        app:layout_constraintStart_toEndOf="@+id/ic_search_history"
        app:layout_constraintTop_toTopOf="@+id/ic_search_history"/>
    <ImageView
        android:id="@+id/ic_search_history_expand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/ic_search_history_expand"
        android:layout_marginEnd="20dp"
        android:layout_marginVertical="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:tint="@color/black_1A1A1A_40"/>
</androidx.constraintlayout.widget.ConstraintLayout>
